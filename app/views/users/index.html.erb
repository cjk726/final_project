 <div class="container">


    <div class="row">
  <div class="col-md-8 col-md-offset-2">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">
          All Users
        </h3>
      </div>
      
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th># of Outfits</th>
      </tr>

      <% @users.each do |user| %>
      <tr>
        <td>
          <a href="/users/<%= user.id %>">
            <%= user.email %>
          </a>
        </td>
        <td><%= user.outfits.count %></td>
      </tr>
      <% end %>
    </table>
      
      
      
      
    </div>
  </div>
</div>